@model UserContext;

@{
    ViewData["Title"] = "Sign In";
}

<table class="table table-hover text-center">
    <thead>
        <tr class="table-dark">
            <th scope="col">Username</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @{
        foreach(var thing in Model.Users){
            <tr>
                <td scope="row">@thing.Username</td>
                <td scope="row">@thing.Email</td>
            </tr>
            }
        }
    </tbody>
</table>